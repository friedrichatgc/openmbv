<test xmlns="http://www.amm.mw.tu-muenchen.de/YYY"
  xmlns:pv="http://www.amm.mw.tu-muenchen.de/XXX/physicalvariable"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.amm.mw.tu-muenchen.de/YYY test.xsd">
  <vt0 unit="ms">
    [1;2;3]
