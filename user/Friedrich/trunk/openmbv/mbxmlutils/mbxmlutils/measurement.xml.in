<?xml version="1.0" encoding="UTF-8"?>
<measurement xmlns="http://openmbv.berlios.de/MBXMLUtils/measurement"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://openmbv.berlios.de/MBXMLUtils/measurement @DATADIR@/@PACKAGE@/schema/measurement.xsd">
  <measure name="nounit" SIunit="">
    <unit name="">value</unit>
  </measure>
  <measure name="length" SIunit="m">
    <unit name="mum">value*1e-6</unit>
    <unit name="mm">value*1e-3</unit>
    <unit name="cm">value*1e-2</unit>
    <unit name="dm">value*1e-1</unit>
    <unit name="m">value</unit>
    <unit name="km">value*1e3</unit>
  </measure>
  <measure name="mass" SIunit="kg">
    <unit name="mg">value*1e-6</unit>
    <unit name="g">value*1e-3</unit>
    <unit name="kg">value</unit>
    <unit name="t">value*1e3</unit>
  </measure>
  <measure name="time" SIunit="s">
    <unit name="mus">value*1e-6</unit>
    <unit name="ms">value*1e-3</unit>
    <unit name="s">value</unit>
    <unit name="sec">value</unit>
    <unit name="min">value*60</unit>
    <unit name="h">value*3600</unit>
    <unit name="d">value*86400</unit>
  </measure>
  <measure name="angle" SIunit="rad">
    <unit name="s">value*3600*180/pi</unit>
    <unit name="sec">value*3600*180/pi</unit>
    <unit name="min">value*60*180/pi</unit>
    <unit name="rad">value</unit>
    <unit name="degree">value*180/pi</unit>
    <unit name="Â°">value*180/pi</unit>
  </measure>
</measurement>
