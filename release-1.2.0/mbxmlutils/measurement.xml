<?xml version="1.0" encoding="UTF-8"?>
<measurement xmlns="http://openmbv.berlios.de/MBXMLUtils/measurement">
  <measure name="nounit" SIunit="-">
    <unit name="">value</unit>
    <unit name="-">value</unit>
    <unit name="%">value/100</unit>
  </measure>
  <!-- SI basis units -->
  <measure name="length" SIunit="m">
    <unit name="mum">value*1e-6</unit>
    <unit name="mm">value*1e-3</unit>
    <unit name="cm">value*1e-2</unit>
    <unit name="dm">value*1e-1</unit>
    <unit name="m">value</unit>
    <unit name="km">value*1e3</unit>
  </measure>
  <measure name="mass" SIunit="kg">
    <unit name="mg">value*1e-6</unit>
    <unit name="g">value*1e-3</unit>
    <unit name="kg">value</unit>
    <unit name="t">value*1e3</unit>
  </measure>
  <measure name="time" SIunit="s">
    <unit name="mus">value*1e-6</unit>
    <unit name="ms">value*1e-3</unit>
    <unit name="s">value</unit>
    <unit name="sec">value</unit>
    <unit name="min">value*60</unit>
    <unit name="h">value*3600</unit>
    <unit name="d">value*86400</unit>
  </measure>
  <measure name="current" SIunit="A">
    <unit name="A">value</unit>
  </measure>
  <measure name="temperature" SIunit="K">
    <unit name="K">value</unit>
    <unit name="degC">value+273.16</unit>
    <unit name="degF">(value+459.67)*5/9</unit>
  </measure>
  <measure name="amount" SIunit="mol">
    <unit name="mol">value</unit>
  </measure>
  <measure name="illumination" SIunit="cd">
    <unit name="cd">value</unit>
  </measure>
  <!-- powers of SI basis units -->
  <measure name="area" SIunit="m^2">
    <unit name="mum^2">value*1e-6^2</unit>
    <unit name="mm^2">value*1e-3^2</unit>
    <unit name="cm^2">value*1e-2^2</unit>
    <unit name="dm^2">value*1e-1^2</unit>
    <unit name="m^2">value</unit>
    <unit name="ar^2">value*1e1^2</unit>
    <unit name="ha">value*1e2^2</unit>
    <unit name="km^2">value*1e3^2</unit>
  </measure>
  <measure name="volume" SIunit="m^3">
    <unit name="mum^3">value*1e-6^3</unit>
    <unit name="mm^3">value*1e-3^3</unit>
    <unit name="cm^3">value*1e-2^3</unit>
    <unit name="dm^3">value*1e-1^3</unit>
    <unit name="l">value*1e-1^3</unit>
    <unit name="m^3">value</unit>
    <unit name="km^3">value*1e3^3</unit>
  </measure>
  <!-- derived SI units -->
  <measure name="angle" SIunit="rad">
    <unit name="rad">value</unit>
    <unit name="degree">value/360*2*pi</unit>
  </measure>
  <measure name="force" SIunit="N">
    <unit name="mN">value*1e-3</unit>
    <unit name="N">value</unit>
    <unit name="kN">value*1e3</unit>
  </measure>
  <measure name="pressure" SIunit="Pa">
    <unit name="mPa">value*1e-3</unit>
    <unit name="Pa">value</unit>
    <unit name="mbar">value*1e2</unit>
    <unit name="bar">value*1e5</unit>
  </measure>
  <!-- other units -->
  <measure name="velocity" SIunit="m/s">
    <unit name="m/s">value</unit>
    <unit name="km/h">value*1e3/3600</unit>
  </measure>
  <measure name="acceleration" SIunit="m/s^2">
    <unit name="m/s^2">value</unit>
  </measure>
  <measure name="flow" SIunit="m^3/s">
    <unit name="mm^3/s">value*1e-3^3/1</unit>
    <unit name="m^3/s">value</unit>
    <unit name="l/s">value*1e-1^3/1</unit>
    <unit name="l/min">value*1e-1^3/60</unit>
  </measure>
  <measure name="density" SIunit="kg/m^3">
    <unit name="kg/m^3">value</unit>
    <unit name="g/cm^3">value*1e-3/1e-2^3</unit>
    <unit name="t/m^3">value*1e3/1^3</unit>
  </measure>
  <measure name="inertia" SIunit="kg*m^2">
    <unit name="kg*m^2">value</unit>
    <unit name="kg*mm^2">value*1*1e-3^2</unit>
    <unit name="g*mm^2">value*1e-3*1e-3^2</unit>
  </measure>
  <measure name="torque" SIunit="N*m">
    <unit name="N*cm">value*1e-2</unit>
    <unit name="N*m">value</unit>
  </measure>
  <measure name="stiffness" SIunit="N/m">
    <unit name="N/m">value</unit>
    <unit name="N/mm">value*1/1e-3</unit>
  </measure>
  <measure name="damping" SIunit="N*s/m">
    <unit name="N*s/m">value</unit>
  </measure>
  <measure name="bulkModulus" SIunit="N/m^2">
    <unit name="N/m^2">value</unit>
    <unit name="N/mm^2">value*1/1e-3^2</unit>
  </measure>
  <measure name="kinematicViscosity" SIunit="m^2/s">
    <unit name="m^2/s">value</unit>
    <unit name="mm^2/s">value*1e-3^2/1</unit>
  </measure>
  <measure name="dynamicViscosity" SIunit="Pa*s">
    <unit name="mPa*s">value*1e-3*1</unit>
    <unit name="Pa*s">value</unit>
  </measure>
</measurement>
