<?xml version="1.0" encoding="UTF-8"?>
<Group name="mygrp" xmlns="http://openmbv.berlios.de/OpenMBV"
  xmlns:pv="http://openmbv.berlios.de/MBXMLUtils/physicalvariable"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://openmbv.berlios.de/OpenMBV /home/markus/project/MBSim/openmbv/schema/openmbv.xsd"
  xmlns:xi="http://www.w3.org/2001/XInclude">
  <Cuboid name="mycubeaa">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <length>[1;1;1]</length>
  </Cuboid>
  <xi:include href="mygrp.mysubgrp.ombv.xml"/>
  <Cuboid name="mycube3">
    <hdf5Link ref="mysubgrp/mycubeX"/>
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <length>[1;1;1]</length>
  </Cuboid>
  <Cube name="mycube">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <length>1</length>
  </Cube>
  <Frame name="myframe">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <size>1</size>
    <offset>1</offset>
  </Frame>
  <Arrow name="myarrow">
    <diameter>1</diameter>
    <headDiameter>0.5</headDiameter>
    <headLength>0.75</headLength>
    <type>"toHead"</type>
    <scaleLength>1</scaleLength>
  </Arrow>
  <Frustum name="mycylinder">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <baseRadius>1</baseRadius>
    <topRadius>1</topRadius>
    <height>2</height>
    <innerBaseRadius>0</innerBaseRadius>
    <innerTopRadius>0</innerTopRadius>
  </Frustum>
  <Sphere name="mysphere">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <radius>1</radius>
  </Sphere>
  <Extrusion name="myextrusion">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <windingRule>"odd"</windingRule>
    <height>1</height>
    <contour>
      [ 0.6, 0.2, 0;
        0.6, 0.2, 0;
        0.6, 0.2, 0 ]
    </contour>
    <contour>
      [ 0.6, 0.2, 0;
        0.6, 0.2, 0;
        0.6, 0.2, 0 ]
    </contour>
  </Extrusion>
  <Rotation name="myrotation">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <contour>
      [ 0.6, 0.2, 0;
        0.6, 0.2, 0;
        0.6, 0.2, 0 ]
    </contour>
  </Rotation>
  <InvisibleBody name="myinvisiblebody">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
  </InvisibleBody>
  <CoilSpring name="mycoilspring">
    <numberOfCoils>0</numberOfCoils>
    <springRadius>1</springRadius>
    <crossSectionRadius>0.1</crossSectionRadius>
    <scaleFactor>1</scaleFactor>
  </CoilSpring>
  <CompoundRigidBody name="mycrg">
    <initialTranslation>[0;0;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <Rotation name="myrotation">
      <initialTranslation>
        <pv:xmlVector>
          <pv:ele>2.6</pv:ele>
          <pv:ele>2.7</pv:ele>
          <pv:ele>2.4*7</pv:ele>
        </pv:xmlVector>
      </initialTranslation>
      <initialRotation>ret=load('testv.dat')</initialRotation>
      <scaleFactor>1</scaleFactor>
      <contour>
        [ 0.6, 0.2, 0;
          0.6, 0.2, 0;
          0.6, 0.2, 0 ]
      </contour>
    </Rotation>
  </CompoundRigidBody>
</Group>
